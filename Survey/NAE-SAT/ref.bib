}

@incollection{avidorImprovedApproximationAlgorithms2006,
  title = {Improved {{Approximation Algorithms}} for {{MAX NAE-SAT}} and {{MAX SAT}}},
  booktitle = {Approximation and {{Online Algorithms}}},

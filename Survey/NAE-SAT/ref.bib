@online{boulebnaneSolvingBooleanSatisfiability2022,
  title = {Solving Boolean Satisfiability Problems with the Quantum Approximate Optimization Algorithm},
  author = {Boulebnane, Sami and Montanaro, Ashley},
  date = {2022-08-14},
  eprint = {2208.06909},
  eprinttype = {arXiv},
  eprintclass = {quant-ph},
  url = {http://arxiv.org/abs/2208.06909},
  urldate = {2024-10-10},
  abstract = {The quantum approximate optimization algorithm (QAOA) is one of the most prominent proposed applications for near-term quantum computing. Here we study the ability of QAOA to solve hard constraint satisfaction problems, as opposed to optimization problems. We focus on the fundamental boolean satisfiability problem, in the form of random k-SAT. We develop analytic bounds on the average success probability of QAOA over random boolean formulae at the satisfiability threshold, as the number of variables n goes to infinity. The bounds hold for fixed parameters and when k is a power of 2. We complement these theoretical results with numerical results on the performance of QAOA for small n, showing that these match the limiting theoretical bounds closely. We then use these results to compare QAOA with leading classical solvers. In the case of random 8-SAT, we find that for around 14 ansatz layers, QAOA matches the scaling performance of the highest-performance classical solver we tested, WalkSATlm. For larger numbers of layers, QAOA outperforms WalkSATlm, with an ultimate level of advantage that is still to be determined. Our methods provide a framework for analysing the performance of QAOA for hard constraint satisfaction problems and finding further speedups over classical algorithms.},
  langid = {english},
  pubstate = {prepublished},
  keywords = {ccfInfo:,citationNumber: undefined,Quantum Physics}
}

@online{brakensiekMysteriesMAXNAESAT2024,
  title = {On the {{Mysteries}} of {{MAX NAE-SAT}}},
  author = {Brakensiek, Joshua and Huang, Neng and Potechin, Aaron and Zwick, Uri},
  date = {2024-09-26},
  eprint = {2009.10677},
  eprinttype = {arXiv},
  url = {http://arxiv.org/abs/2009.10677},
  urldate = {2024-10-10},
  abstract = {MAX NAE-SAT is a natural optimization problem, closely related to its better-known relative MAX SAT. The approximability status of MAX NAE-SAT is almost completely understood if all clauses have the same size \$k\$, for some \$k\textbackslash ge 2\$. We refer to this problem as MAX NAE-\$\textbackslash\{k\textbackslash\}\$-SAT. For \$k=2\$, it is essentially the celebrated MAX CUT problem. For \$k=3\$, it is related to the MAX CUT problem in graphs that can be fractionally covered by triangles. For \$k\textbackslash ge 4\$, it is known that an approximation ratio of \$1-\textbackslash frac\{1\}\{2\textasciicircum\{k-1\}\}\$, obtained by choosing a random assignment, is optimal, assuming \$P\textbackslash ne NP\$. For every \$k\textbackslash ge 2\$, an approximation ratio of at least \$\textbackslash frac\{7\}\{8\}\$ can be obtained for MAX NAE-\$\textbackslash\{k\textbackslash\}\$-SAT. There was some hope, therefore, that there is also a \$\textbackslash frac\{7\}\{8\}\$-approximation algorithm for MAX NAE-SAT, where clauses of all sizes are allowed simultaneously. Our main result is that there is no \$\textbackslash frac\{7\}\{8\}\$-approximation algorithm for MAX NAE-SAT, assuming the unique games conjecture (UGC). In fact, even for almost satisfiable instances of MAX NAE-\$\textbackslash\{3,5\textbackslash\}\$-SAT (i.e., MAX NAE-SAT where all clauses have size \$3\$ or \$5\$), the best approximation ratio that can be achieved, assuming UGC, is at most \$\textbackslash frac\{3(\textbackslash sqrt\{21\}-4)\}\{2\}\textbackslash approx 0.8739\$. Using calculus of variations, we extend the analysis of O'Donnell and Wu for MAX CUT to MAX NAE-\$\textbackslash\{3\textbackslash\}\$-SAT. We obtain an optimal algorithm, assuming UGC, for MAX NAE-\$\textbackslash\{3\textbackslash\}\$-SAT, slightly improving on previous algorithms. The approximation ratio of the new algorithm is \$\textbackslash approx 0.9089\$. We complement our theoretical results with some experimental results. We describe an approximation algorithm for almost satisfiable instances of MAX NAE-\$\textbackslash\{3,5\textbackslash\}\$-SAT with a conjectured approximation ratio of 0.8728, and an approximation algorithm for almost satisfiable instances of MAX NAE-SAT with a conjectured approximation ratio of 0.8698.},
  langid = {american},
  pubstate = {prepublished},
  keywords = {ccfInfo: CCF-A SODA,citationNumber: 6,Computer Science - Computational Complexity,Computer Science - Data Structures and Algorithms}
}


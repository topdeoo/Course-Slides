@online{changUpperBounds$2$colorability2023,
  title = {Upper Bounds on the \$2\$-Colorability Threshold of Random \$d\$-Regular \$k\$-Uniform Hypergraphs for \$k\textbackslash geq 3\$},
  author = {Chang, Evan and Kolhe, Neel and Sohn, Youngtak},
  date = {2023-08-03},
  eprint = {2308.02075},
  eprinttype = {arXiv},
  url = {http://arxiv.org/abs/2308.02075},
  urldate = {2024-10-10},
  abstract = {For a large class of random constraint satisfaction problems (CSP), deep but non-rigorous theory from statistical physics predict the location of the sharp satisfiability transition. The works of Ding, Sly, Sun (2014, 2016) and Coja-Oghlan, Panagiotou (2014) established the satisfiability threshold for random regular \$k\$-NAE-SAT, random \$k\$-SAT, and random regular \$k\$-SAT for large enough \$k\textbackslash geq k\_0\$ where \$k\_0\$ is a large non-explicit constant. Establishing the same for small values of \$k\textbackslash geq 3\$ remains an important open problem in the study of random CSPs. In this work, we study two closely related models of random CSPs, namely the \$2\$-coloring on random \$d\$-regular \$k\$-uniform hypergraphs and the random \$d\$-regular \$k\$-NAE-SAT model. For every \$k\textbackslash geq 3\$, we prove that there is an explicit \$d\_\{\textbackslash ast\}(k)\$ which gives a satisfiability upper bound for both of the models. Our upper bound \$d\_\{\textbackslash ast\}(k)\$ for \$k\textbackslash geq 3\$ matches the prediction from statistical physics for the hypergraph \$2\$-coloring by Dall'Asta, Ramezanpour, Zecchina (2008), thus conjectured to be sharp. Moreover, \$d\_\{\textbackslash ast\}(k)\$ coincides with the satisfiability threshold of random regular \$k\$-NAE-SAT for large enough \$k\textbackslash geq k\_0\$ by Ding, Sly, Sun (2014).},
  langid = {american},
  pubstate = {prepublished},
  keywords = {ccfInfo: CCF-C ?ED:100000,citationNumber: undefined,Computer Science - Discrete Mathematics,Mathematical Physics,Mathematics - Combinatorics,Mathematics - Mathematical Physics,Mathematics - Probability}
}

@online{dingSatisfiabilityThresholdRandom2013,
  title = {Satisfiability Threshold for Random Regular {{NAE-SAT}}},
  author = {Ding, Jian and Sly, Allan and Sun, Nike},
  date = {2013-10-17},
  eprint = {1310.4784},
  eprinttype = {arXiv},
  url = {http://arxiv.org/abs/1310.4784},
  urldate = {2024-10-10},
  abstract = {We consider the random regular \$k\$-NAE-SAT problem with \$n\$ variables each appearing in exactly \$d\$ clauses. For all \$k\$ exceeding an absolute constant \$k\_0\$, we establish explicitly the satisfiability threshold \$d\_*=d\_*(k)\$. We prove that for \$d{$<$}d\_*\$ the problem is satisfiable with high probability while for \$d{$>$}d\_*\$ the problem is unsatisfiable with high probability. If the threshold \$d\_*\$ lands exactly on an integer, we show that the problem is satisfiable with probability bounded away from both zero and one. This is the first result to locate the exact satisfiability threshold in a random constraint satisfaction problem exhibiting the condensation phenomenon identified by Krzakala et al. (2007). Our proof verifies the one-step replica symmetry breaking formalism for this model. We expect our methods to be applicable to a broad range of random constraint satisfaction problems and combinatorial problems on random graphs.},
  langid = {american},
  pubstate = {prepublished},
  keywords = {ccfInfo: CCF-A STOC,citationNumber: 29,Mathematical Physics,Mathematics - Mathematical Physics,Mathematics - Probability}
}

@online{el-kadiQuantumApproximateOptimisation2024,
  title = {Quantum {{Approximate Optimisation}} for {{Not-All-Equal SAT}}},
  author = {El-Kadi, Andrew and Bondesan, Roberto},
  date = {2024-01-05},
  eprint = {2401.02852},
  eprinttype = {arXiv},
  url = {http://arxiv.org/abs/2401.02852},
  urldate = {2024-10-10},
  abstract = {Establishing quantum advantage for variational quantum algorithms is an important direction in quantum computing. In this work, we apply the Quantum Approximate Optimisation Algorithm (QAOA) -- a popular variational quantum algorithm for general combinatorial optimisation problems -- to a variant of the satisfiability problem (SAT): Not-All-Equal SAT (NAE-SAT). We focus on regimes where the problems are known to have solutions with low probability and introduce a novel classical solver that outperforms existing solvers. Extensively benchmarking QAOA against this, we show that while the runtime of both solvers scales exponentially with the problem size, the scaling exponent for QAOA is smaller for large enough circuit depths. This implies a polynomial quantum speedup for solving NAE-SAT.},
  langid = {american},
  pubstate = {prepublished},
  keywords = {ccfInfo: CCF-C ?ED:100000,citationNumber: undefined,Quantum Physics}
}

